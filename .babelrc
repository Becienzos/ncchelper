{
    "presets": [
        ["@babel/preset-env", {
            "targets": "defaults",
            "modules": false,
            "loose": true
        }],
        "@babel/preset-flow"
    ],
    "plugins": [
        ["@babel/plugin-transform-runtime", { "useESModules": true }],
        ["@babel/plugin-proposal-class-properties", { "loose": true }],
        [
            "@babel/plugin-transform-react-jsx",
            {
                "pragma": "m",
                "pragmaFrag": "'['",
                "useBuiltIns": true
            }
        ]
    ]
}
