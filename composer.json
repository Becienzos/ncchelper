{
    "name": "devcode-it/openstamanager",
    "description": "Gestionale open-source per assistenza tecnica e fatturazione elettronica",
    "license": "GPL-3.0",
    "keywords": [
        "gestionale",
        "assistenza tecnica",
        "fatturazione elettronica",
        "open-source"
    ],
    "homepage": "https://www.openstamanager.com/",
    "authors": [{
        "name": "DevCode s.n.c.",
        "email": "info@openstamanager.com"
    }],
    "type": "project",
    "require": {
        "php": "^7.3|^8.0",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-fileinfo": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-simplexml": "*",
        "ext-xsl": "*",
        "ext-zip": "*",
        "barryvdh/laravel-debugbar": "^3.5",
        "danielstjules/stringy": "^3.1",
        "devcode-it/aggiornamenti": "@dev",
        "devcode-it/causali-trasporto": "@dev",
        "devcode-it/legacy": "@dev",
        "fideloper/proxy": "^4.4",
        "filp/whoops": "^2.1",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.3",
        "laravel/framework": "^8.12",
        "laravel/tinker": "^2.5",
        "symfony/polyfill-ctype": "^1.8",
        "symfony/polyfill-php70": "^1.8",
        "zerospam/laravel-gettext": "^7.2"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.0",
        "jeroen-g/laravel-packager": "^2.5",
        "laravel/homestead": "^12.2",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "jeroen-g/laravel-packager"
            ]
        }
    },
    "repositories": {
        "devcode-it/aggiornamenti": {
            "type": "path",
            "url": "./packages/devcode-it/aggiornamenti",
            "options": {
                "symlink": true
            }
        },
        "devcode-it/causali-trasporto": {
            "type": "path",
            "url": "./packages/devcode-it/causali-trasporto",
            "options": {
                "symlink": true
            }
        },
        "devcode-it/legacy": {
            "type": "path",
            "url": "../legacy",
            "options": {
                "symlink": true
            }
        }
    }
}
