{
  "name": "devcode-it/openstamanager",
  "type": "project",
  "description": "Gestionale open-source per assistenza tecnica e fatturazione elettronica",
  "license": "GPL-3.0",
  "keywords": [
    "gestionale",
    "assistenza tecnica",
    "fatturazione elettronica",
    "open-source"
  ],
  "homepage": "https://www.openstamanager.com/",
  "authors": [
    {
      "name": "DevCode s.r.l.",
      "email": "info@openstamanager.com",
      "homepage": "https://www.devcode.it/",
      "role": "Developer"
    },
    {
      "name": "Thomas Zilio (Dasc3er)",
      "role": "Contributor"
    },
    {
      "name": "Maicol Battistini (maicol07)",
      "email": "webmaster@maicol07.it",
      "homepage": "https://maicol07.it",
      "role": "Contributor"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-pdo": "*",
    "binaryk/laravel-restify": "^8",
    "guzzlehttp/guzzle": "^7",
    "inertiajs/inertia-laravel": "^0",
    "jackiedo/dotenv-editor": "^2",
    "laravel/fortify": "*",
    "laravel/framework": "^10",
    "laravel/sanctum": "^3",
    "laravel/tinker": "^2",
    "nette/utils": "^4",
    "opcodesio/log-viewer": "^2",
    "outhebox/blade-flags": "^1",
    "rawilk/laravel-settings": "^2",
    "spatie/file-system-watcher": "^1",
    "tightenco/ziggy": "^1"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3",
    "doctrine/dbal": "^3.6",
    "driftingly/rector-laravel": "^0",
    "fakerphp/faker": "^1",
    "friendsofphp/php-cs-fixer": "^3",
    "fumeapp/modeltyper": "^2.1",
    "glhd/laravel-dumper": "^1",
    "laravel-lang/attributes": "^2",
    "laravel-lang/lang": "^12",
    "laravel-lang/publisher": "^14",
    "laravel/pint": "^1",
    "nunomaduro/collision": "^7",
    "nunomaduro/larastan": "^2",
    "rector/rector": "^0",
    "roave/security-advisories": "dev-latest",
    "spatie/laravel-ignition": "^2",
    "thiagocordeiro/laravel-translator": "^1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi",
      "@php artisan vendor:publish --tag=blade-flags --tag=log-viewer-assets --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
