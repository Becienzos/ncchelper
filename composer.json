{
  "name": "devcode-it/openstamanager",
  "type": "project",
  "description": "Gestionale open-source per assistenza tecnica e fatturazione elettronica",
  "license": "GPL-3.0",
  "keywords": [
    "gestionale",
    "assistenza tecnica",
    "fatturazione elettronica",
    "open-source"
  ],
  "homepage": "https://www.openstamanager.com/",
  "authors": [
    {
      "name": "DevCode s.r.l.",
      "email": "info@openstamanager.com",
      "homepage": "https://www.devcode.it/",
      "role": "Developer"
    },
    {
      "name": "Thomas Zilio (Dasc3er)",
      "role": "Contributor"
    },
    {
      "name": "Maicol Battistini (maicol07)",
      "email": "webmaster@maicol07.it",
      "homepage": "https://maicol07.it",
      "role": "Contributor"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-pdo": "*",
    "akaunting/laravel-setting": "^1.2",
    "guzzlehttp/guzzle": "^7.4",
    "inertiajs/inertia-laravel": "^0.5",
    "innocenzi/laravel-vite": "^0.2",
    "laravel-json-api/laravel": "^2.1",
    "laravel/framework": "^9.0",
    "laravel/tinker": "^2.7",
    "nette/utils": "^3.2",
    "spatie/file-system-watcher": "^1.1",
    "tightenco/ziggy": "^1.4",
    "wikimedia/composer-merge-plugin": "^2.0"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.6",
    "barryvdh/laravel-ide-helper": "^2.12",
    "friendsofphp/php-cs-fixer": "^3.6",
    "kkomelin/laravel-translatable-string-exporter": "^1.16",
    "nunomaduro/collision": "^6.1",
    "rector/rector": "^0.14.5",
    "roave/security-advisories": "dev-latest",
    "spatie/laravel-ignition": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "wikimedia/composer-merge-plugin": true
    }
  },
  "extra": {
    "merge-plugin": {
      "include": [
        "./composer.local.json"
      ],
      "replace": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
